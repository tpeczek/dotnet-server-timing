{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Lib.ServerTiming.Abstractions/Lib.ServerTiming.Abstractions.csproj",
            "src/Lib.AspNetCore.ServerTiming/Lib.AspNetCore.ServerTiming.csproj",
            "src/Lib.Azure.Functions.Worker.ServerTiming/Lib.Azure.Functions.Worker.ServerTiming.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../.."
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net6.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "toc.md",
          "index.md",
          "articles/getting-started-aspnetcore.md",
          "articles/getting-started-azurefunctions.md",
          "articles/advanced.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "resources/svg/logo.svg",
          "resources/ico/favicon.ico",
          "resources/basic-metrics-values.png",
          "resources/timing-blocks-of-code.png",
          "resources/timing-nested-blocks-of-code.png"
        ]
      }
    ],
    "dest": "wwwroot",
    "globalMetadata": {
      "_appTitle": "Server Timing API support for .NET",
      "_appFooter": "Copyright © 2017 - 2024 Tomasz Pęczek",
      "_appLogoPath": "resources/svg/logo.svg",
      "_appFaviconPath": "resources/ico/favicon.ico",
      "_disableBreadcrumb": true,
      "_disableAffix": true,
      "_disableContribution": true
    }
  }
}